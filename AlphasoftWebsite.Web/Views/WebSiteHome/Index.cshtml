                                        <div class="review-text">
                                            <p>@item.ClientReview</p>
                                        </div>
                                        <div class="review-author d-flex align-items-center">
                                            <div class="review-author-img">
                                                <img src="@Url.Content(image3)" alt="">
                                            </div>
                                            <div class="review-author-info">
                                                <ul class="list-inline">
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                </ul>
                                                <span><strong>@item.ClientName</strong></span>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- End of Single Review -->
                                }
                             
                            </div>
                        </div>

                        <!-- Arrows -->
                        <div class="swiper-button-next next-review">
                            <img src="~/Content/Frontend/img/icons/right-arrow.svg" alt="" class="svg">
                        </div>
                        <div class="swiper-button-prev prev-review">
