<section>
    <div class="main-slider swiper-container">
        <div class="swiper-wrapper">
            <!-- Single slide -->
            @foreach (var item in ViewBag.MainBanner)
            {
                if (item.HomeBannerImage != null)
                {
                    image = imgString + item.HomeBannerImage;
                }

                <div class="swiper-slide position-relative">
                    <img src="@Url.Content(image)" data-rjs="2" alt="">
                    <div class="slide-content container">
                        <div class="row align-items-center">
                            <div class="col-xl-6 col-lg-8">
                                <div class="slide-content-inner">
                                    <h4 data-animate="fadeInUp" data-delay=".05">@item.HomeBannerHeader</h4>
                                    <h2 data-animate="fadeInUp" data-delay=".3">@item.HomeBannerDescription</h2>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            }
            <!-- End of Single slide -->

        </div>
        <!-- Banner Pagination -->
        <div class="swiper-pagination main-slider-pagination"></div>
    </div>
</section>
<!-- End of Banner -->
